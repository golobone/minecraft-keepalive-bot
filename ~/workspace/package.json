{
  "name": "minecraft-keepalive-bot",
  "version": "2.0.0",
  "description": "Bot de Minecraft con Aternos y notificaciones Discord.",
  "main": "bot.js",
  "scripts": {
    "start": "node bot.js",
    "launcher": "node launcher.js",
    "build:windows": "pkg launcher.js -t node18-win-x64 -o MCBotApp.exe --compress Brotli"
  },
  "pkg": {
    "scripts": ["bot.js", "launcher.js"],
    "assets": [".env", "src/**/*"]
  },
  "dependencies": {
    "axios": "^1.6.0",
    "dotenv": "^16.0.0",
    "mineflayer": "^4.20.1"
  },
  "devDependencies": {
    "pkg": "^5.8.1"
  }
}
